module main

import lang/lexer
import lang/parser
import std/vec2
import std/maths
import std/rect

fn main()
    let str = "12 + 7-4 * 33"

    let (tokens, rest) = lexer::lexTokens(str)
    print(tokens, rest)

    let (n, ts) = parser::parseExpr(tokens)
    print(n, ts)

    let v = Vec2(3.0, 4.0)

    print(v + Vec2(31.0, 2.0) )
    print(length(v))

    print( maths::sqrt( f32(3))  )
    print( maths::sqrt( 3.3 )  )
    print( f32(3) )

    let r = Rect( Vec2(20.3, 1.0), v)
    print(r)

