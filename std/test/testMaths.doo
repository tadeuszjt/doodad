module testMaths

import ../maths
import ../io

fn floatsEqual(a f64, b f64) bool
    let error = 0.00000001
    return (a - b) > (-error) && (a - b) < error


fn testSqrt()
    let true = floatsEqual(4, maths::sqrt(16))
    let false = floatsEqual(4, maths::sqrt(16.01))
    let true = floatsEqual(1, maths::sqrt(1))
    let false = floatsEqual(1, maths::sqrt(-1))
    let true = floatsEqual(0.70710678, maths::sqrt(0.5))


fn {io Io} testMaths()
    testSqrt()
    io.write("testMaths passed\n")

