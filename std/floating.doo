module floating

fn set(a &F64, b F64); ${ *$a = $b; }

fn construct(a F64) F64
    let b : F64
    ${ $b = $a; }
    return b

fn add(a F64, b F64) F64
    let r : F64
    ${ $r = $a + $b; }
    return r

fn divide(a F64, b F64) F64
    let r : F64
    ${ $r = $a / $b; }
    return r


fn equal(a F64, b F64) Bool
    let r : Bool
    ${ $r = $a == $b; }
    return r


