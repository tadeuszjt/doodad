module floating

import std/features

fn set(a &F64, b F64); ${ *$a = $b; }

fn Construct::construct(a F64) F64
    let b : F64
    ${ $b = $a; }
    return b

fn Arithmetic::add(a F64, b F64) F64
    let r : F64
    ${ $r = $a + $b; }
    return r

fn Arithmetic::divide(a F64, b F64) F64
    let r : F64
    ${ $r = $a / $b; }
    return r


fn Compare::equal(a F64, b F64) Bool
    let r : Bool
    ${ $r = $a == $b; }
    return r


