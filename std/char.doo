module char

import builtin/all

fn eof() Char
    let c : Char
    ${ $c = EOF; }
    return c


fn isAlpha(c Char) Bool
    let r : Bool
    ${ $r = isalpha($c); }
    return r


fn isDigit(c Char) Bool
    let r = false
    ${ $r = isdigit($c); }
    return r


fn isSpace(c Char) Bool
    let r = false
    ${ $r = isspace($c); }
    return r
