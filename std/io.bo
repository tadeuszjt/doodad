module io

import_c stdio.h

fn getChar() char
    return char(c::getchar())

fn getStrLn() string
    let str = ""

    while true
        let c = char(c::getchar())
        if c < 1;          return "\0"
        else if c == '\n'; return str
        else;              str <- [c]

fn putStrLn(str string)
    let i = 0
    while i < len(str)
        c::putchar(i64(str[i]))
        i = i + 1

    c::putchar(i64('\n')) 

